<h1>Lista de suppliers</h1>
<div class="span7">
<center>
<%= render 'layouts/searcher' %>
<%= will_paginate %>
<% @suppliers.each do |sup| %>
      <h5>
      	<table border="1" style="width:100%">
      	<tr>
        <td><%= link_to sup.fantasy_name_supplier, '/suppliers/'+sup.id.to_s %>
        - CNPJ: <%= sup.cnpj %></td>
      </tr>
      </h5>

</table>
<% end %>
<%= will_paginate %>
</center>
</div>
